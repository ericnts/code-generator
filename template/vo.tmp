package {Package}

import (
	"{Project}/common"
	"{Project}/entity"{Import}
)

type {Object}Query struct {
	common.Query

{Field}
}

type {Object} struct {
{Field}
}

func (v *{Object}) Load(inter common.EntityInter) {
	e, ok := inter.(*entity.{Object})
	if !ok {
		return
	}
{ToVO}
}

type {Object}Detail struct {
	{Object}
}

func (v *{Object}Detail) Load(inter common.EntityInter) {
	e, ok := inter.(*entity.{Object})
	if !ok {
		return
	}
	v.{Object}List = {Object}List{}
	v.{Object}List.Load(e)
}

type {Object}Modify struct {
{Field}
}

func (p *{Object}Modify) ToEntity(id string) common.EntityInter {
	return &entity.{Object}{
		Entity: common.Entity{
			ID: id,
		},
{ToEntity}
	}
}

type {Object}Create struct {
	{Object}Modify
}

func (p *{Object}Create) ToEntity() common.EntityInter {
	return p.{Object}Modify.ToEntity("")
}
